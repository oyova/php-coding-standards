{
    "name": "oyova/php-coding-standards",
    "description": "A wrapper for PHP CS Fixer.",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Oyova\\PhpCodingStandards\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "friendsofphp/php-cs-fixer": "^3.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
